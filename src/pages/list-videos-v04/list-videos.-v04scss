page-list-videos-v04 {
  .footerSpace{
      display: block;
      margin:auto;
      margin-bottom:60px;
  }

  .swiper-container{
      height: auto;
  }
  h3{
      text-transform: uppercase;
      font-size: 16px;
  }

  .listAulas{

    transition: .5;
    max-height:0px !important;
    overflow: hidden;

    &.show{
      max-height:none !important;

    }
  }
  .boxGallery{
      position: relative;

      hr{
          margin-top:15px;
          margin-bottom:30px;
          margin-right:40px;
          margin-left:10px;

      }

      .nextVideo{
          display: block;
          position: absolute;
          top: 34%;
          font-size: 50px;
          right: -9px;
          z-index: 9999;
          display:none;
      }
      .prevVideo{
          display: block;
          position: absolute;
          top: 34%;
          font-size: 50px;
          left: -9px;
          z-index: 9999;
          cursor: pointer;
          display: none;
      }

  }


  .itemProject{
      *{
          padding: 0px;
          margin:0px;
      }
      h2{
          text-transform: uppercase;
          font-weight: bold;
          padding-top:20px;
      }
       p{
          font-size: 14px;
          margin-top: 5px;
          margin-bottom: 10px;
      }
      .item-md.item-block .item-inner{
          padding-right:0px;
      }
      .slideVideo{
          margin-top: 20px;
          margin-left: 20px;
          position: relative;
          margin-bottom: 18px;

          img{
              padding:0px;
          }
          h2{
              text-align: left;
              font-size: 15px;
              padding: 0px;
              margin: 0px;
              padding-top:3px;

          }
          p{
              font-size: 14px;
              font-style: italic;

              white-space: normal;
              text-align: justify;
              line-height: 1.5;
          }
      }

  }
}

